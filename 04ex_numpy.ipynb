    "        for p in range(i+1,98):\n",
    "            if(N[p]%N[i]==0):\n",
    "                N[p]=False\n",
    "mask=(N!=0)\n",
    "filtered=N[mask]\n",
    "print(\"Prime numbers\\n\",filtered)"
