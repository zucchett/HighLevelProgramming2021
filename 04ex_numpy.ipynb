   "source": [
    "u = np.array([1, 3, 5, 7])\n",
    "v = np.array([2, 4, 6, 8])\n",
    "print(\"u:\",u)\n",
    "print(\"v:\",v)\n",
    "print(\"\\n\")\n",
    "\n",
    "# 1)\n",
    "output =np.outer(u, v)\n",
    "print(\"Using the function outer in numpy\")\n",
    "print(output,\"\\n\")\n",
    "\n",
    "# 2)\n",
    "m = np.zeros((u.shape[0],v.shape[0]))\n",
    "\n",
    "print(\"Using a nested for loop\")\n",
    "\n",
    "for i in range(u.shape[0]):\n",
    "    for j in range(v.shape[0]):\n",
    "        m[i,j] = u[i]*v[j]\n",
    "\n",
    "print(m,\"\\n\")\n",
    "\n",
    "# 3)\n",
    "print(\"Using numpy broadcasting operations\")\n",
    "\n",
    "x = u.reshape(4,1)\n",
    "y = v.reshape(1,4)\n",
    "\n",
    "print(x*y)\n"
