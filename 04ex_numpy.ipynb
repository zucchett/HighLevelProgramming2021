    "def sieve_of_eratosthenes(N):\n",
    "    is_prime = []\n",
    "    for b in range(N+1):\n",
    "        b = True\n",
    "        is_prime.append(b)\n",
    "        \n",
    "    is_prime[0], is_prime[1] = False, False\n",
    "\n",
    "    for i in range(2, int(N**0.5)+1):\n",
    "        for j in range(2*i, N+1, i):\n",
    "            is_prime[j] = False\n",
    "            \n",
    "    primes=[]\n",
    "    for i in range(2, N+1):\n",
    "        if is_prime[i] == True:\n",
    "            primes.append(i)\n",
    "    \n",
    "    return primes\n",
    "\n",
    "%time primes = sieve_of_eratosthenes(99)\n",
    "print(\"N = 99:\",primes)\n",
    "\n",
    "%time primes = sieve_of_eratosthenes(99)"
