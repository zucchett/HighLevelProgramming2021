   "source": [
    "import numpy as np \n",
    "import math\n",
    "m = np.arange(12).reshape((3,4))\n",
    "print(m)\n",
    "print (\"mean:\", np.mean(m))\n",
    "\n",
    "print(\"mean along rows\",m.mean(axis=1))\n",
    "\n",
    "print(\"mean along column\",m.mean(axis=0))\n",
    "\n"
