    "def prime(N):\n",
    "    primes = []\n",
    "    for i in range(2,N+1):\n",
    "        flag = False\n",
    "        for j in range(2,i):\n",
    "            if i % j == 0:\n",
    "                flag = True\n",
    "                break\n",
    "        if flag == False:\n",
    "            primes.append(i)\n",
    "    return primes\n",
