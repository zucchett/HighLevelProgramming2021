    "import math as m\n",
    "import time\n",
    "\n",
    "def f_xI(x):\n",
    "    return m.sqrt(1-x**2)\n",
    "\n",
    "def integral_semiCircle(N):\n",
    "    st_t = time.time() #Strat time\n",
    "    sum = 0\n",
    "    h = 2/N\n",
    "    st_p = -1 #Start point\n",
    "    for i in range(1,N+1):\n",
    "        \n",
    "        sum += (h*f_xI(st_p))\n",
    "        st_p += h\n",
    "    f_t = time.time() - st_t #Total time\n",
    "    return sum, f_t\n",
    "\n",
    "def maxN():\n",
    "    N = 10000000\n",
    "    while True:\n",
    "        I_N, t_N = integral_semiCircle(N)\n",
    "        print(t_N)\n",
    "        if(t_N >= 60):\n",
    "            return I_N, t_N, N\n",
    "        N += 1000000 \n",
