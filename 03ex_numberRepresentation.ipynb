    "import sys\n",
    "\n",
    "def binDecHex(x):\n",
    "    x_bin = bin(x)\n",
    "    x_hex = hex(x)\n",
    "    x_dec = int(x)\n",
    "    return x_bin, x_hex, x_dec\n",
    "\n",
    "x = 0x5101\n",
    "\n",
    "a, b, c = binDecHex(x)\n",
    "\n",
    "print(\"Binary representation: \" + a)\n",
    "print(\"Hexadecimal representation: \" + b)\n",
    "print(\"Decimal representation: \" + str(c))"
   ]
